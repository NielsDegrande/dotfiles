{
    "workbench.colorTheme": "Solarized Dark",
    "window.zoomLevel": 0,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "editor.fontSize": 14,
    "editor.fontFamily": "'JetBrains Mono', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
    "workbench.startupEditor": "newUntitledFile",
    "files.autoSave": "afterDelay",
    "files.insertFinalNewline": true,
    "workbench.editor.showTabs": true,
    "[python]": {
        "editor.rulers": [
            72,
            79
        ]
    },
    "python.formatting.blackArgs": [
        "--line-length=79"
    ],
    "cSpell.userWords": [
        "CPUs",
        "POSIX",
        "Unmarshalling",
        "ansible",
        "astype",
        "bazel",
        "bresenham",
        "brewfile",
        "chipset",
        "discretize",
        "dotfiles",
        "dtype",
        "fillna",
        "frecency",
        "groupby",
        "hspace",
        "iloc",
        "inplace",
        "isin",
        "isna",
        "issubset",
        "itertools",
        "journaling",
        "mackup",
        "malloc",
        "multithreaded",
        "multithreading",
        "mutex",
        "mutexes",
        "ndarray",
        "niels",
        "numpy",
        "nunique",
        "parallelization",
        "pylint",
        "pyplot",
        "pytest",
        "quadrotor",
        "reindex",
        "roadmap",
        "rwlock",
        "scipy",
        "setp",
        "sociodemographics",
        "softmax",
        "stateful",
        "swappable",
        "tolist",
        "trello",
        "unmarshal",
        "voxel",
        "xdist",
        "xlabel",
        "xlsx"
    ],
    "files.associations": {
        "*.rmd": "markdown",
        "*.Brewfile": "shellscript"
    },
    "cSpell.enableFiletypes": [
        "r"
    ],
    "[markdown]": {
        "editor.tabSize": 2
    },
    "python.formatting.provider": "black",
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.linting.enabled": true,
    "python.pythonPath": "/usr/local/bin/python3",
    "telemetry.enableCrashReporter": false,
    "gitlens.advanced.telemetry.enabled": false,
    "telemetry.enableTelemetry": false,
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[sql]": {
        "editor.tabSize": 2
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "sonarlint.rules": {
        "python:S139": {
            "level": "on"
        },
        "python:S1717": {
            "level": "on"
        },
        "python:S2733": {
            "level": "on"
        },
        "python:S2712": {
            "level": "on"
        },
        "python:ClassComplexity": {
            "level": "on"
        },
        "python:S134": {
            "level": "on"
        },
        "python:S1720": {
            "level": "on"
        },
        "python:S113": {
            "level": "on"
        },
        "python:FileComplexity": {
            "level": "on"
        },
        "python:S104": {
            "level": "on"
        },
        "python:FunctionComplexity": {
            "level": "on"
        },
        "python:S1142": {
            "level": "on"
        },
        "python:S5439": {
            "level": "on"
        },
        "python:LineLength": {
            "level": "on"
        },
        "python:S1131": {
            "level": "on"
        },
        "python:LongIntegerWithLowercaseSuffixUsage": {
            "level": "on"
        },
        "python:S2325": {
            "level": "on"
        },
        "python:S1578": {
            "level": "on"
        },
        "python:S1722": {
            "level": "off"
        },
        "python:S1721": {
            "level": "on"
        },
        "python:ParsingError": {
            "level": "on"
        },
        "python:OneStatementPerLine": {
            "level": "on"
        },
        "python:S1707": {
            "level": "on"
        }
    },
    "python.languageServer": "Pylance"
}
