{
  "editor.fontSize": 14,
  "editor.fontFamily": "'JetBrains Mono', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.lineHeight": 24,
  "errorLens.excludePatterns": ["**/*.{md}"],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.Brewfile": "shellscript"
  },
  "files.autoSave": "afterDelay",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/env/**": true,
    "**/node_modules/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "files.insertFinalNewline": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.sendSelectionToInteractiveWindow": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "**/bower_components": true,
    "**/env": true,
    "**/node_modules": true,
    "**/venv": true
  },
  "security.workspace.trust.enabled": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.editor.decorations.colors": false,
  "workbench.editor.decorations.badges": false,
  "workbench.editor.showTabs": true,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",

  "cSpell.userWords": [
    "CPUs",
    "POSIX",
    "Unmarshalling",
    "ansible",
    "asctime",
    "asdict",
    "astype",
    "bazel",
    "bresenham",
    "brewfile",
    "chipset",
    "coloredlogs",
    "cyclomatic",
    "dataclasses",
    "dataframes",
    "decentral",
    "degrande",
    "discretize",
    "docstrings",
    "dotfiles",
    "dropna",
    "dtype",
    "dtypes",
    "evar",
    "fillna",
    "frecency",
    "groupby",
    "gunicorn",
    "hardcode",
    "hardcoding",
    "hspace",
    "iloc",
    "inplace",
    "isin",
    "isna",
    "isort",
    "issubset",
    "iter",
    "iterr",
    "iterrows",
    "itertools",
    "journaling",
    "kpis",
    "levelname",
    "mackup",
    "malloc",
    "mlflow",
    "mssql",
    "multithreaded",
    "multithreading",
    "mutex",
    "mutexes",
    "mypy",
    "ndarray",
    "niels",
    "nrows",
    "numpy",
    "nunique",
    "openpyxl",
    "parallelization",
    "post",
    "postgres",
    "postgresql",
    "pydantic",
    "pylint",
    "pymssql",
    "pyodbc",
    "pyplot",
    "pystan",
    "pytest",
    "quadrotor",
    "reindex",
    "roadmap",
    "rwlock",
    "scip",
    "scipy",
    "setp",
    "setuptools",
    "sklearn",
    "sociodemographics",
    "softmax",
    "stateful",
    "struct",
    "sublanguages",
    "swappable",
    "timeit",
    "tolist",
    "trello",
    "typer",
    "unmarshal",
    "uvicorn",
    "vectorize",
    "voxel",
    "xdist",
    "xgboost",
    "xlabel",
    "xlsx",
    "xticks"
  ],
  "cSpell.enableFiletypes": ["r"],
  "cSpell.language": "en,en-US",

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[markdown]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[python]": {
    "editor.rulers": [72, 79]
  },
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.pythonPath": "/usr/local/bin/python3",
  "autoDocstring.docstringFormat": "sphinx",
  "autoDocstring.guessTypes": false,
  "autoDocstring.customTemplatePath": "/Users/degrandeniels/git/dotfiles/mackup/.vscode/sphinx_no_types.mustache",

  "[sql]": {
    "editor.tabSize": 2
  },

  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
}
