{
  "editor.fontFamily": "'JetBrainsMono Nerd Font', Menlo, Monaco, 'Courier New', monospace",
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 20,
  "editor.suggestSelection": "first",
  "errorLens.excludePatterns": ["**/*.{md}"],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.Brewfile": "shellscript"
  },
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/env/**": true,
    "**/node_modules/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "files.insertFinalNewline": true,
  "git.mergeEditor": true,
  "jupyter.askForKernelRestart": false,
  "jupyter.sendSelectionToInteractiveWindow": true,
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "**/bower_components": true,
    "**/env": true,
    "**/node_modules": true,
    "**/venv": true
  },
  "security.workspace.trust.enabled": false,
  "telemetry.telemetryLevel": "off",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.commandCenter": true,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.editor.decorations.colors": false,
  "workbench.editor.decorations.badges": false,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile",

  "cSpell.userWords": [
    "ansible",
    "aptering",
    "arange",
    "asctime",
    "asdict",
    "astype",
    "azureml",
    "bazel",
    "bresenham",
    "brewfile",
    "chipset",
    "coloredlogs",
    "CPUs",
    "cyclomatic",
    "dataclasses",
    "dataframe",
    "dataframes",
    "decentral",
    "degrande",
    "discretize",
    "docstrings",
    "dotfiles",
    "dropna",
    "dtype",
    "dtypes",
    "evar",
    "fastapi",
    "fillna",
    "financials",
    "Formik",
    "frecency",
    "groupby",
    "gunicorn",
    "hardcode",
    "hardcoding",
    "hspace",
    "iloc",
    "inplace",
    "isin",
    "isna",
    "isort",
    "issubset",
    "iter",
    "iterr",
    "iterrows",
    "itertools",
    "journaling",
    "kpis",
    "levelname",
    "mackup",
    "malloc",
    "MILP",
    "mlflow",
    "mssql",
    "multithreaded",
    "multithreading",
    "mutex",
    "mutexes",
    "mypy",
    "ndarray",
    "niels",
    "nrows",
    "numpy",
    "nunique",
    "odbc",
    "openpyxl",
    "ortools",
    "parallelization",
    "POSIX",
    "post",
    "postgres",
    "postgresql",
    "pydantic",
    "pylint",
    "pymssql",
    "pyodbc",
    "pyplot",
    "pystan",
    "pytest",
    "pywraplp",
    "quadrotor",
    "redoc",
    "reindex",
    "roadmap",
    "rwlock",
    "scip",
    "scipy",
    "setp",
    "setuptools",
    "sklearn",
    "sociodemographics",
    "softmax",
    "starlette",
    "stateful",
    "struct",
    "sublanguages",
    "swappable",
    "timeit",
    "timespan",
    "tolist",
    "trello",
    "typer",
    "unmarshal",
    "Unmarshalling",
    "unsanitize",
    "uvicorn",
    "vectorize",
    "voxel",
    "xdist",
    "xgboost",
    "xlabel",
    "xlsx",
    "xticks"
  ],
  "cSpell.enableFiletypes": ["r"],
  "cSpell.language": "en,en-US",

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[javascriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "always",

  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[markdown]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[python]": {
    "editor.rulers": [72, 79]
  },
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.mypyEnabled": true,
  "python.linting.pylintEnabled": false,
  "python.testing.pytestArgs": ["--no-cov"],
  "python.testing.pytestEnabled": true,
  "autoDocstring.customTemplatePath": "/Users/degrandeniels/git/dotfiles/mackup/.vscode/sphinx_no_types.mustache",
  "autoDocstring.docstringFormat": "sphinx",
  "autoDocstring.guessTypes": false,
  "autoDocstring.startOnNewLine": true,

  "[sql]": {
    "editor.tabSize": 2
  },

  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[typescriptreact]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",

  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tabnine.experimentalAutoImports": true,
  "editor.stickyScroll.enabled": true,
}
