{
    "workbench.colorTheme": "Solarized Dark",
    "window.zoomLevel": 0,
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "editor.fontSize": 14,
    "editor.fontFamily": "'JetBrains Mono', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
    "workbench.startupEditor": "newUntitledFile",
    "files.autoSave": "afterDelay",
    "files.insertFinalNewline": true,
    "workbench.editor.showTabs": true,
    "[python]": {
        "editor.rulers": [
            72,
            79
        ]
    },
    "python.formatting.blackArgs": [
        "--line-length=79"
    ],
    "cSpell.userWords": [
        "CPUs",
        "POSIX",
        "Unmarshalling",
        "ansible",
        "astype",
        "bazel",
        "brewfile",
        "chipset",
        "dotfiles",
        "dtype",
        "fillna",
        "frecency",
        "groupby",
        "hspace",
        "iloc",
        "inplace",
        "isin",
        "isna",
        "issubset",
        "itertools",
        "journaling",
        "mackup",
        "malloc",
        "multithreaded",
        "multithreading",
        "mutex",
        "mutexes",
        "ndarray",
        "niels",
        "numpy",
        "nunique",
        "parallelization",
        "pyplot",
        "pytest",
        "reindex",
        "rwlock",
        "scipy",
        "setp",
        "stateful",
        "swappable",
        "tolist",
        "unmarshal",
        "xdist",
        "xlabel"
    ],
    "files.associations": {
        "*.rmd": "markdown",
        "*.Brewfile": "shellscript"
    },
    "cSpell.enableFiletypes": [
        "r"
    ],
    "[markdown]": {
        "editor.tabSize": 2
    },
    "python.formatting.provider": "black"
}
